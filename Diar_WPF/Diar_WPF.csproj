<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="diary_journal_icon.ico" />
    <None Remove="Images\diary_journal_icon.ico" />
    <None Remove="Images\refresh_icon.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="diary_journal_icon.ico" />
    <Resource Include="Images\diary_journal_icon.ico">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Images\refresh_icon.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

</Project>
